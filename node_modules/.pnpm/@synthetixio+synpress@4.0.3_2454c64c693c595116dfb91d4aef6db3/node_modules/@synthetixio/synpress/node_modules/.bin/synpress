#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/synpress/dist/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/synpress/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/synpress/dist/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/synpress/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules/@synthetixio/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress@4.0.3_2454c64c693c595116dfb91d4aef6db3/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
