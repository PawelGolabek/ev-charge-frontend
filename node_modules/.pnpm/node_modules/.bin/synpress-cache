#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/dist/cli/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/dist/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/dist/cli/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/dist/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/synpress-cache/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules/@synthetixio/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/@synthetixio+synpress-cache_374bdef6dd2ca85294f37bf157b548f9/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@synthetixio/synpress-cache/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../@synthetixio/synpress-cache/dist/cli/index.js" "$@"
fi
