#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/bin/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules:/mnt/c/Users/pigeo/Desktop/GRID/frontend/EVCharge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../.pnpm/webpack-dev-server@5.0.4_webpack@5.94.0_esbuild@0.19.12_/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
